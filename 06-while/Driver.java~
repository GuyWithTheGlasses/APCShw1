public class Driver {
    public static void main(String[] args) {
	While test = new While();
	test.frontTimes("WELCOME",5); //should be "WELWELWELWELWEL"
	test.stringBits("p1o2t3a4t5o6"); //should be "potato"
	test.stringYak("I love yaks"); //should be "I love s"
	test.stringMatch("A dog!","A donut!"); //should be 3
    }
}
